<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/_5sdp"
        android:padding="@dimen/_5sdp"
        tools:ignore="UselessParent">

        <ImageView
            android:layout_width="@dimen/_60sdp"
            android:layout_height="@dimen/_60sdp"
            android:elevation="@dimen/_1sdp"
            android:src="@drawable/ananta_circle"
            tools:ignore="ContentDescription">

        </ImageView>

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/home_category_image"
            android:layout_width="@dimen/_55sdp"
            android:layout_height="@dimen/_55sdp"
            android:layout_marginStart="3.5dp"
            android:layout_marginTop="3.2dp"
            android:padding="@dimen/_2sdp"
            android:src="@drawable/ananta"
            tools:ignore="MissingConstraints" />

    </FrameLayout>

    <TextView
        android:id="@+id/home_category_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text=""
        android:layout_marginTop="@dimen/_3sdp"
        android:textColor="#303030"
        android:textSize="@dimen/_9ssp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout4"
        app:layout_constraintStart_toStartOf="@+id/frameLayout4"
        app:layout_constraintTop_toBottomOf="parent"></TextView>

    </LinearLayout>
