<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="MybottomTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="MybottomTextAppearanceinactive" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">#90F5F5F5</item>
    </style>
    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView" tools:ignore="MissingPrefix">
        <item name="android:queryHint">"What are you looking for?"</item>
        <item name="android:textColor">#BC252424</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColorHint">#BC3A3737</item>
        <item name="android:editTextColor">#BC252424</item>
        <item name="android:buttonCornerRadius" tools:ignore="NewApi">@dimen/_110sdp</item>

    </style>

    <style name="FullScreen" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="aleartdialog" parent="Theme.AppCompat.Light.Dialog.Alert" tools:ignore="NewApi">
        <item name="background">@color/white</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat" tools:ignore="MissingPrefix">
        <item name="colorControlNormal">#2196F3</item>
        <item name="colorControlActivated">#FFC107</item>
    </style>

    <style name="Menutheme" parent="Theme.AppCompat.Light" tools:ignore="NewApi">
        <item name="android:colorBackground">#FFFF</item>
        <item name="android:textColor">#E1201E1E</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="iconPadding">@dimen/_1sdp</item>
        <item name="android:listPreferredItemHeightSmall">40dp</item>
    </style>

</resources>